# .env.example for sop-maker
CHATBOT_BASE_URL=https://openrouter.ai/api/v1/chat/completions
CHATBOT_MODEL=arcee-ai/trinity-large-preview:free
OPENROUTER_API_KEY=your-openrouter-api-key
# MongoDB connection string
MONGODB_URI=mongodb://localhost:27017/sop-maker

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_INFO_BASE_URL=https://www.googleapis.com/oauth2/v3/userinfo

# Session secret
SESSION_SECRET=your-session-secret

# Frontend URL
BASE_URL=http://localhost:3000
FRONTEND_URL=http://localhost:3000

# Port
PORT=3000

# Other environment variables as needed
# ...
